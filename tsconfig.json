{
  "include": ["src/index.js"],
  "compilerOptions": {
    "outDir": "typings-autogen",
    "module": "ESNext",
    "target": "ES2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "lib": ["ESNext"],
    "checkJs": true,
    "allowJs": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "stripInternal": true,
  },
  "exclude": [
    "node_modules",
    "**/node_modules/*",
  ],
}